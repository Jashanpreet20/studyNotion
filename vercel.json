{
   
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "/https://study-notion-eta-jet.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
    ,
    "builds":[
        {
          "src":"./server/server.js",
          "use":"@vercel/node"
        }
    ]
  }

  
    
  
  